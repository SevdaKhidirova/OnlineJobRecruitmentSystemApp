@{
    Layout = null;
}

@model Link

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">
    <link rel="shortcut icon" href="~/assets/images/favicon1.webp" type="image/x-icon">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;600;900&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="~/assets/css/registeration.css">
    <title>Job</title>
</head>
<body>
    <section id="register-title">
        <div class="row">
            <div class="col-xl-3 col-lg-4  col-md-6 col-sm-12 ">
                <partial name="_EditLinksPartialView" />
            </div>
            <div class="col-xl-9 col-lg-8 col-md-6 col-sm-12  pt-5 right">
                <h4 class="px-4 contact-form title">Contact Infotmation</h4>
                <div class="personal-inf m-2 p-2 px-4  mt-lg-3">
                    <form method="post">
                        <div class="mb-3 ">
                            <label for="exampleInputLinkedin" class="form-label">Linkedin URL</label>
                            <input type="text" placeholder="Add url" class="form-control p-3" asp-for="Linkedin" value="@Model.Linkedin" id="exampleInputLinkedin" aria-describedby="nameHelp" >
                        </div>
                        <div class="mb-3 ">
                            <label for="exampleInputGit" class="form-label">Github URL</label>
                            <input type="text" placeholder="Add url" class="form-control p-3" asp-for="Github" value="@Model.Github" id="exampleInputGit" aria-describedby="surnameHelp" >
                        </div>
                        <div class="mb-3">
                            <label for="exampleInputFacebook" class="form-label">Facebook URL</label>
                            <input type="text" placeholder="Add url" class="form-control p-3" asp-for="Facebook" value="@Model.Facebook" id="exampleInputFacebook" aria-describedby="surnameHelp" >
                        </div>
                        <button type="submit" class="btn btn-primary btn-lg ">Complete</button>
                    </form>

                </div>
                <div class="buttons">
                    <a href="/Profil/InfoAppeal" class="btn btn-light next">Back to Home</a>
                    <a href="/editprofil/language" class="btn btn-light"><i class="bi bi-arrow-left"></i>Back</a>
                </div>
            </div>
        </div>
    </section>







    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-pprn3073KE6tl6bjs2QrFaJGz5/SUsLqktiwsUTF55Jfv3qYSDhgCecCxMW52nD2" crossorigin="anonymous"></script>
    <script src="~/assets/js/main.js"></script>

</body>
</html>